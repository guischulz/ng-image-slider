{
  "name": "ng-image-slider",
  "version": "2.8.1",
  "description": "Angular Image slider with lightbox. An Angular responsive image slider with lightbox popup. Also support youtube url, image base64 string and mp4 video urls and base64 string.",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4100 --host 0.0.0.0",
    "build": "ng build ng-image-slider",
    "watch": "ng build --watch",
    "build:prod": "ng build ng-image-slider --configuration production",
    "test": "ng test"
  },
  "files": [
    "dist/ng-image-slider"
  ],
  "dependencies": {
    "@angular/animations": "~13.2.1",
    "@angular/common": "~13.2.1",
    "@angular/compiler": "~13.2.1",
    "@angular/core": "~13.2.1",
    "@angular/forms": "~13.2.1",
    "@angular/platform-browser": "~13.2.1",
    "@angular/platform-browser-dynamic": "~13.2.1",
    "@angular/router": "~13.2.1",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.2",
    "@angular/cli": "~13.2.2",
    "@angular/compiler-cli": "~13.2.1",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.20.43",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.2.1",
    "typescript": "~4.5.5"
  }
}
